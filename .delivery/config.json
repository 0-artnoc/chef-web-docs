{
  "version": "2",
  "job_dispatch": {
    "version": "v2",
    "filters": {
      "default": {
        "os": ["linux"],
        "platform": ["ubuntu"],
        "platform_version": ["14.04"]
      }
    }
  },
  "build_cookbook": {
      "name": "build-cookbook",
      "path": ".delivery/build-cookbook"
  },
  "skip_phases": [
    "functional"
    "lint",
    "quality",
    "security",
    "syntax",
    "unit"
  ]
}
